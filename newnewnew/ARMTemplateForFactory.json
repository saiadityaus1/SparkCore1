{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "newnewnew"
		},
		"NEWlinkedService1_clusterPassword": {
			"type": "secureString",
			"metadata": "Secure string for 'clusterPassword' of 'NEWlinkedService1'"
		},
		"NEWlinkedService1_clusterSshPassword": {
			"type": "secureString",
			"metadata": "Secure string for 'clusterSshPassword' of 'NEWlinkedService1'"
		},
		"NEWlinkedService1_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'NEWlinkedService1'"
		},
		"newAzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'newAzureBlobStorage1'"
		},
		"NEWlinkedService1_properties_typeProperties_hostSubscriptionId": {
			"type": "string",
			"defaultValue": "7eaba0ec-4235-43fc-b940-3b78739b2573"
		},
		"NEWlinkedService1_properties_typeProperties_clusterResourceGroup": {
			"type": "string",
			"defaultValue": "pres"
		},
		"NEWlinkedService1_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "e6075603-e386-4026-a332-3e932e605b2c"
		},
		"NEWlinkedService1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "0b7d2613-2a2e-4a10-b3b5-2b9fd01fac78"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/pipeline1')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "newSpark1",
						"type": "HDInsightSpark",
						"dependsOn": [],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"rootPath": "pcontainer",
							"entryFilePath": "SparkMaven.jar",
							"sparkJobLinkedService": {
								"referenceName": "newAzureBlobStorage1",
								"type": "LinkedServiceReference"
							}
						},
						"linkedServiceName": {
							"referenceName": "NEWlinkedService1",
							"type": "LinkedServiceReference"
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/newAzureBlobStorage1')]",
				"[concat(variables('factoryId'), '/linkedServices/NEWlinkedService1')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/NEWlinkedService1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "NEWlinkedService1",
				"annotations": [],
				"type": "HDInsightOnDemand",
				"typeProperties": {
					"clusterType": "hadoop",
					"clusterSize": 1,
					"timeToLive": "00:15:00",
					"version": "4.0",
					"hostSubscriptionId": "[parameters('NEWlinkedService1_properties_typeProperties_hostSubscriptionId')]",
					"clusterResourceGroup": "[parameters('NEWlinkedService1_properties_typeProperties_clusterResourceGroup')]",
					"servicePrincipalId": "[parameters('NEWlinkedService1_properties_typeProperties_servicePrincipalId')]",
					"tenant": "[parameters('NEWlinkedService1_properties_typeProperties_tenant')]",
					"clusterNamePrefix": "",
					"clusterPassword": {
						"type": "SecureString",
						"value": "[parameters('NEWlinkedService1_clusterPassword')]"
					},
					"clusterSshPassword": {
						"type": "SecureString",
						"value": "[parameters('NEWlinkedService1_clusterSshPassword')]"
					},
					"servicePrincipalKey": {
						"type": "SecureString",
						"value": "[parameters('NEWlinkedService1_servicePrincipalKey')]"
					},
					"additionalLinkedServiceNames": [
						{
							"referenceName": "newAzureBlobStorage1",
							"type": "LinkedServiceReference"
						}
					],
					"linkedServiceName": {
						"referenceName": "newAzureBlobStorage1",
						"type": "LinkedServiceReference"
					},
					"headNodeSize": "",
					"dataNodeSize": "",
					"zookeeperNodeSize": ""
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/newAzureBlobStorage1')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/newAzureBlobStorage1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('newAzureBlobStorage1_connectionString')]"
				}
			},
			"dependsOn": []
		}
	]
}